<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cloudstaff.actionscan" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icons/android/drawable-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="res/icons/android/drawable-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="res/icons/android/drawable-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/icons/android/drawable-xhdpi/ic_launcher.png" />
    <name>Action-Scan</name>
    <description>
    A QR scanner application For cloudstaff.
  </description>
    <author email="jr.evilboss@gmail.com" href="https://github.com/evilboss">
    Jr Reyes
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
